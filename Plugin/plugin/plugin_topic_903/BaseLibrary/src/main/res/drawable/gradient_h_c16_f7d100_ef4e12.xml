<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners android:radius="16dp" />
            <gradient
                android:angle="0"
                android:endColor="#FFEF4E12"
                android:startColor="#FFF7D100"
                android:type="linear" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="16dp"/>
            <stroke
                android:width="1dp"
                android:color="#FFFE9D" />
        </shape>
    </item>
</layer-list>