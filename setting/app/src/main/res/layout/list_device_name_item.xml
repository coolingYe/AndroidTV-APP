<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/device_name_layout"
    android:layout_width="564dp"
    android:layout_height="56dp"
    android:layout_gravity="center_horizontal"
    android:layout_marginLeft="15dp"
    android:layout_marginTop="7dp"
    android:background="@drawable/bg_wifi_detail_selector"
    android:focusable="true">

    <TextView
        android:id="@+id/device_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:duplicateParentState="true"
        android:text="自定义设备名称（AI互动屏盒子）"
        android:textColor="@drawable/selector_text_bg"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_device_check"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginRight="12dp"
        android:duplicateParentState="true"
        android:src="@drawable/bg_checked_selector"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
