<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <padding
                android:left="3dp"
                android:top="3dp"
                android:right="3dp"
                android:bottom="3dp" />
            <corners android:radius="10dp"/>
            <stroke
                android:width="1dp"
                android:color="#00E8FF" />
        </shape>
    </item>
</layer-list>

